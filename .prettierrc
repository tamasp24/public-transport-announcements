{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"htmlWhitespaceSensitivity": "ignore",
	"svelteStrictMode": true,
	"overrides": [
		{
			"files": ["*.svelte", "*.ts", "*.js", "*.html", "*.css"],
			"options": {
				"parser": "svelte"
			}
		}
	]
}
